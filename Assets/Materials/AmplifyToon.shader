			half4 frag ( VertexOutput IN  ) : SV_Target
			{
				UNITY_SETUP_INSTANCE_ID( IN );
				UNITY_SETUP_STEREO_EYE_INDEX_POST_VERTEX( IN );

				#if defined(ASE_NEEDS_FRAG_WORLD_POSITION)
				float3 WorldPosition = IN.worldPos;
				#endif
				float4 ShadowCoords = float4( 0, 0, 0, 0 );

				#if defined(ASE_NEEDS_FRAG_SHADOWCOORDS)
					#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
						ShadowCoords = IN.shadowCoord;
					#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
						ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
					#endif
				#endif
				Gradient gradient24 = NewGradient( 1, 3, 2, float4( 0.241, 0.241, 0.241, 0.2382391 ), float4( 0.383, 0.383, 0.383, 0.5000076 ), float4( 1, 1, 1, 1 ), 0, 0, 0, 0, 0, float2( 1, 0 ), float2( 1, 1 ), 0, 0, 0, 0, 0, 0 );
				float3 ase_worldNormal = IN.ase_texcoord3.xyz;
				float3 normalizedWorldNormal = normalize( ase_worldNormal );
				float dotResult1 = dot( normalizedWorldNormal , SafeNormalize(_MainLightPosition.xyz) );
				float normal_lightdir19 = dotResult1;
				float2 uv_AlbedoTex = IN.ase_texcoord4.xy * _AlbedoTex_ST.xy + _AlbedoTex_ST.zw;
				float4 Albedo42 = ( _Tint * tex2D( _AlbedoTex, uv_AlbedoTex ) );
				float4 shadow28 = ( SampleGradient( gradient24, (normal_lightdir19*_Offset + _Offset) ) * Albedo42 );
				float3 bakedGI50 = ASEIndirectDiffuse( IN.lightmapUVOrVertexSH.xy, ase_worldNormal);
				float ase_lightAtten = 0;
				Light ase_lightAtten_mainLight = GetMainLight( ShadowCoords );
				ase_lightAtten = ase_lightAtten_mainLight.distanceAttenuation * ase_lightAtten_mainLight.shadowAttenuation;
				float4 lighting49 = ( shadow28 * ( _MainLightColor * float4( ( bakedGI50 + ase_lightAtten ) , 0.0 ) ) );
				float3 ase_worldViewDir = ( _WorldSpaceCameraPos.xyz - WorldPosition );
				ase_worldViewDir = SafeNormalize( ase_worldViewDir );
				float dotResult10 = dot( normalizedWorldNormal , ase_worldViewDir );
				float normal_viewdir20 = dotResult10;
				float4 Rim62 = ( saturate( ( pow( ( 1.0 - saturate( ( normal_viewdir20 + _RimOffset ) ) ) , _RimPower ) * ( normal_lightdir19 * ase_lightAtten ) ) ) * ( _MainLightColor * _RimTint ) );
				float dotResult79 = dot( ( ase_worldViewDir + _MainLightPosition.xyz ) , normalizedWorldNormal );
				float smoothstepResult82 = smoothstep( _Min , _Max , pow( dotResult79 , _SpecSize ));
				float2 uv_SpecMap = IN.ase_texcoord4.xy * _SpecMap_ST.xy + _SpecMap_ST.zw;
				float4 Specular89 = ( ( ( smoothstepResult82 * tex2D( _SpecMap, uv_SpecMap ) ) * _SpecIntensity ) * ase_lightAtten );
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = ( ( lighting49 + Rim62 ) + Specular89 ).rgb;
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

				#ifdef _ALPHATEST_ON
					clip( Alpha - AlphaClipThreshold );
				#endif

				#ifdef LOD_FADE_CROSSFADE
					LODDitheringTransition( IN.clipPos.xyz, unity_LODFade.x );
				#endif

				#ifdef ASE_FOG
					Color = MixFog( Color, IN.fogFactor );
				#endif

				return half4( Color, Alpha );
			}

			ENDHLSL
